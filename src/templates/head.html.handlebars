<meta name="viewport" content="initial-scale=1">
<link rel="icon" type="image/png" href="/icons/icon-64.png">
<link href="/atom.xml" type="application/atom+xml" rel="alternate" title="ATOM Feed">
<title>{{title}}</title>
<script src="/index.js" async></script>
<link href="{{cssPath}}" rel="stylesheet">
<link href="https://twitter.com/qubyte" rel="me">
<link href="https://github.com/qubyte" rel="me">
<link href="https://mastodon.social/@qubyte" rel="me">
{{#isBlogEntry}}
{{#if prevLink}}
<link href="{{prevLink}}" rel="prev">
{{/if}}
{{#if nextLink}}
<link href="{{nextLink}}" rel="next">
{{/if}}
<link href="/webmention" rel="webmention">
<meta name="theme-color" content="#f1e7cd">
<meta name="twitter:card" content="summary">
<meta property="og:type" content="blog">
<meta name="twitter:site" content="@qubyte">
<meta name="twitter:title" property="og:title" content="{{attributes.title}}">
<meta name="twitter:description" property="og:description" content="{{attributes.description}}">
<meta name="twitter:image" property="og:image" content="https://qubyte.codes/icons/photo-512.jpg">
<meta name="twitter:image:alt" content="The qubyte.codes site icon.">
{{/isBlogEntry}}
{{#dev}}
<script>
  new EventSource('/events').addEventListener('new', () => {
    console.log('RELOAD');
    window.location.reload();
  });
</script>
{{/dev}}
