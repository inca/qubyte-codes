{
  "name": "qubyte-codes",
  "version": "1.0.0",
  "description": "qubyte.codes website",
  "main": "index.js",
  "repository": "github:qubyte/qubyte-codes",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cp hooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "build-css": "cleancss frontend/reset.css frontend/main.css frontend/code-style.css > public/main.css",
    "build": "node -e \"require('.').build()\" && npm run build-css",
    "addbuiltfiles": "git add public",
    "schedule": "echo \"git --git-dir $(PWD)/.git pull origin master\" | at",
    "start": "node serve.js"
  },
  "author": "Mark Stanley Everitt",
  "license": "Private",
  "dependencies": {
    "clean-css": "3.4.8",
    "front-matter": "2.0.1",
    "handlebars": "4.0.5",
    "highlight.js": "9.0.0",
    "marked": "0.3.6",
    "remark": "3.1.0",
    "slug": "0.9.1"
  },
  "devDependencies": {
    "eslint": "3.8.0",
    "eslint-config-qubyte": "1.0.1",
    "toisu": "3.0.3",
    "toisu-static": "1.1.0"
  }
}
