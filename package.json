{
  "name": "qubyte-codes",
  "version": "1.0.0",
  "description": "qubyte.codes website",
  "main": "index.js",
  "repository": "github:qubyte/qubyte-codes",
  "bin": {
    "build-js": "./bin/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cp hooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "build-css": "cleancss frontend/reset.css frontend/main.css frontend/code-style.css > public/main.css",
    "build": "rm -f public/*.css && npm run build-css && ./bin/cli.js . && sw-precache --config=sw-precache-config.js",
    "addbuiltfiles": "git add public",
    "schedule": "echo \"git --git-dir $(PWD)/.git pull origin master\" | at",
    "start": "NODE_ENV=development node serve.js"
  },
  "author": "Mark Stanley Everitt",
  "license": "Private",
  "private": true,
  "dependencies": {
    "clean-css-cli": "4.0.0",
    "front-matter": "2.1.2",
    "handlebars": "4.0.6",
    "highlight.js": "9.12.0",
    "marked": "0.3.6",
    "remark": "6.2.0",
    "slug": "0.9.1",
    "sw-precache": "5.1.0"
  },
  "devDependencies": {
    "chokidar": "1.7.0",
    "eslint": "4.2.0",
    "eslint-config-qubyte": "1.1.0",
    "toisu": "3.0.3",
    "toisu-router": "4.0.0",
    "toisu-static": "1.1.1"
  }
}
