{
  "name": "qubyte-codes",
  "version": "1.0.0",
  "description": "qubyte.codes website",
  "main": "index.js",
  "repository": "github:qubyte/qubyte-codes",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cp hooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "build": "node -e \"require('.').build()\" && uglifyjs frontend/main.js -c -m > public/main.js",
    "addbuiltfiles": "git add public",
    "schedule": "echo \"git --git-dir $(PWD)/.git pull origin master\" | at",
    "start": "http-server"
  },
  "author": "Mark Stanley Everitt",
  "license": "Private",
  "dependencies": {
    "clean-css": "3.4.8",
    "front-matter": "2.0.1",
    "handlebars": "3.0.3",
    "highlight.js": "9.0.0",
    "http-server": "0.8.5",
    "marked": "0.3.5",
    "remark": "3.1.0",
    "slug": "0.9.1",
    "uglify-js": "2.7.3"
  },
  "devDependencies": {
    "eslint": "3.8.0",
    "eslint-config-qubyte": "1.0.1"
  }
}
